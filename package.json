    "eslint": "^9.32.0",
    "lint-staged": "^16.1.4",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.13.3",
    "typescript": "^5.9.2",
    "ufo": "^1.6.1",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }

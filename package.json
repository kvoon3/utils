{
  "name": "@kvoon/utils",
  "type": "module",
  "version": "0.2.3",
  "packageManager": "pnpm@10.14.0",
  "description": "Kevin Kwong's utils",
  "author": "Kevin Kwong <kvoon@duck.com>",
  "license": "MIT",
  "keywords": [],
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
